<?xml version="1.0" encoding="UTF-8"?>
<crux xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
      xsi:noNamespaceSchemaLocation="../../Crux/xsd/crux.xsd"
      module="br.com.sysmap.crux.core.JSEngine">
  <component id="button" clientConstructorParams="id, new com.google.gwt.user.client.ui.Button()" />
  <component id="label" clientConstructorParams="id, new com.google.gwt.user.client.ui.Label()"/>
  <component id="textBox" clientConstructorParams="id, new com.google.gwt.user.client.ui.TextBox()"/>
  <component id="passwordTextBox" clientConstructorParams="id, new com.google.gwt.user.client.ui.PasswordTextBox()"/>
  
  
  <component id="simplePanel" 
  			 clientClass="br.com.sysmap.crux.core.client.component.Container"
             clientConstructorParams="id, new com.google.gwt.user.client.ui.SimplePanel()" 
             serverClass="br.com.sysmap.crux.core.server.screen.Container" />
  <component id="scrollPanel" 
  			 clientClass="br.com.sysmap.crux.core.client.component.Container"
             clientConstructorParams="id, new com.google.gwt.user.client.ui.ScrollPanel()"
             serverClass="br.com.sysmap.crux.core.server.screen.Container" />
  <component id="absolutePanel" 
  			 clientClass="br.com.sysmap.crux.core.client.component.Container"
             clientConstructorParams="id, new com.google.gwt.user.client.ui.AbsolutePanel()"
             serverClass="br.com.sysmap.crux.core.server.screen.Container" />
  <component id="stackPanel" 
  			 clientClass="br.com.sysmap.crux.core.client.component.Container"
             clientConstructorParams="id, new com.google.gwt.user.client.ui.StackPanel()"
             serverClass="br.com.sysmap.crux.core.server.screen.Container" />
             
             
</crux>
