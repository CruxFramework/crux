<x:xdevice xmlns:x="http://www.cruxframework.org/xdevice"
           xmlns:gwt="http://www.cruxframework.org/crux/gwt" 
           useController="slideshowTouchController">
    <gwt:simplePanel id="mainPanel" >
    </gwt:simplePanel>
	<gwt:dialogBox id="dialog" styleName="slideshowDialog">
     	<gwt:focusPanel id="focusPanel" width="100%" height="100%" style="outline:none;" onTouchEnd="slideshowTouchController.onTouchEndDialog"
     					onTouchStart="slideshowTouchController.onTouchStartDialog" onTouchMove="slideshowTouchController.onTouchMoveDialog" >
	     	<gwt:dockPanel id="table" width="100%" height="100%">
	     	</gwt:dockPanel>
     	</gwt:focusPanel>
	</gwt:dialogBox>
</x:xdevice>