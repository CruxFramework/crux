<?xml version="1.0" encoding="UTF-8"?>
<crux xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
      xsi:noNamespaceSchemaLocation="../../Crux/xsd/crux.xsd"
      module="br.com.sysmap.crux.ext.CruxBasicComponents">
  <component id="button" 
             clientClass="br.com.sysmap.crux.ext.client.component.FocusComponent"
  			 clientConstructorParams="id, new com.google.gwt.user.client.ui.Button()" 
  			 />
  <component id="checkBox" 
             clientClass="br.com.sysmap.crux.ext.client.component.CheckBox"
  			 clientConstructorParams="id, new com.google.gwt.user.client.ui.CheckBox()" 
  			 />
  <component id="radioButton" 
             clientClass="br.com.sysmap.crux.ext.client.component.CheckBox"
  			 clientConstructorParams="id, new com.google.gwt.user.client.ui.RadioButton()" 
  			 />
  <component id="textBox" 
             clientClass="br.com.sysmap.crux.ext.client.component.FocusComponent"
             clientConstructorParams="id, new com.google.gwt.user.client.ui.TextBox()"
  			 />
  <component id="label" clientConstructorParams="id, new com.google.gwt.user.client.ui.Label()"/>
  <component id="passwordTextBox" clientConstructorParams="id, new com.google.gwt.user.client.ui.PasswordTextBox()"/>
