  			 clientClass="br.com.sysmap.crux.core.client.component.Container"
             clientConstructorParams="id, new com.google.gwt.user.client.ui.SimplePanel()"  />
  <component id="scrollPanel" 
  			 clientClass="br.com.sysmap.crux.core.client.component.Container"
