  <component id="absolutePanel" 
  			 clientClass="br.com.sysmap.crux.core.client.component.Container"
             clientConstructorParams="id, new com.google.gwt.user.client.ui.AbsolutePanel()" />
  <component id="stackPanel" 
  			 clientClass="br.com.sysmap.crux.core.client.component.Container"
             clientConstructorParams="id, new com.google.gwt.user.client.ui.StackPanel()" />
  <component id="textArea" 
  			 clientClass="br.com.sysmap.crux.ext.client.component.TextArea"
  			 clientConstructorParams="id, new com.google.gwt.user.client.ui.TextArea()" />
  <component id="htmlPanel" 
  			 clientClass="br.com.sysmap.crux.ext.client.component.HTMLContainer"
             clientConstructorParams="id" />
</crux>
